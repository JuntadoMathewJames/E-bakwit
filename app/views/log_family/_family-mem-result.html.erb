<%= form_with url: "/logpeople", method: :post  do |f|%>
  <div class="container-fluid">
    <div class="row">
      <% @families.each do |x| %>
        <div class="col-md-12 p-1 mb-3" style="font-size:18px; box-shadow: 3px 3px 2px 2px black; outline:3px dashed black">
          <div class="form-check" >
                  <input class="form-check-input" type="checkbox" value="<%=x.id%>" id="<%=x.id%>_<%=x.fname %>">
                  <label class="form-check-label w-100 "  for="<%=x.id%>_<%=x.fname %>">
                    <%= x.fname%> <%= x.lname%></label>
                  </label>
          </div>
        </div>
      <% end%>
    </div>
  </div>
<%end%>
